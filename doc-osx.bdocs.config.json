{
	"doxyfilePath": "./Doxyfile-Mac",
	"doxygenOutputPath": "./docs-Mac",
	"websiteRootPath": "~/Projects/icedcoffee-meta/website/",
	"relativeOutputPath": "content/reference/osx/",

	"executeDoxygen": true,
	"executeNanoc": true,
	"cleanIntermediates": true,

	"mainTitle": "Icedcoffee OS X API Reference",
	"version": "0.7",
	
	"classPrefix": "IC",
	"macroPrefix": "IC_",
	
	"generateYAML": true,
	"documentIvars": false,
	
	"nameMap": {
		"anonEnum": {
			"ICHitTest(.*)": "ICHitTestResult",
			"ICControlEvent(.*)": "ICControlEvents",
			"ICAbstractControlEvent(.*)": "ICAbstractControlEvents",
			"IC(.*)MouseButton": "ICMouseButton",
			"ICMouse(.*)": "ICAbstractMouseEventType",
			"IC(.*)Version(.*)": "ICOSVersion",
			"ICLeftMouseDown": "ICOSXEventType"
		}
	}
}